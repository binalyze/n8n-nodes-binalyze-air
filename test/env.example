# Example YAML environment file for local development
# Copy this to .env.local.yml in the root directory and fill in the values

# Credentials for creating an n8n Credential for testing purposes
# You can generate this token at:
# AIR -> Integrations -> API Tokens -> Add New
AIR:
  INSTANCE_URL: https://your-air-instance.binalyze.io
  API_TOKEN: api_xxxxxxxxxxxxxxxxxxxxxxxxxx

# N8N API Token for accessing the local n8n instance
# You can generate this token in your n8n instance at:
# Settings -> API -> Personal access tokens
N8N:
  INSTANCE_URL: http://127.0.0.1:5678
  API_TOKEN: your_n8n_api_token_here
