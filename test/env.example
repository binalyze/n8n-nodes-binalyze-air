# Example YAML environment file for local development
# Rename this file to .env.local.yml

# N8N API Token for accessing the local n8n instance
# You can generate this token in your n8n instance at:
# Settings -> API -> Personal access tokens
N8N:
  INSTANCE_URL: http://127.0.0.1:5678
  API_TOKEN: your_n8n_api_token_here
